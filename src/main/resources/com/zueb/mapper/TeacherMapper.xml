<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zueb.mapper.TeacherMapper">




    <!--    修改学生信息-->
    <update id="update" parameterType="com.zueb.pojo.Teacher">
        update t_teacher set name = #{name},
                             code = #{code},
                             age = #{age},
                             dept = #{dept},
                             address = #{address},
                             flage = #{flage}
                       where id = #{id}
    </update>



</mapper>